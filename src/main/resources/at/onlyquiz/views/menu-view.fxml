<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<VBox fx:id="container" alignment="CENTER" prefHeight="720.0" prefWidth="1280.0" spacing="10.0" styleClass="background"
      xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="at.onlyquiz.controller.MenuController">

    <Button fx:id="playButton" onAction="#pressPlayButton" styleClass="menu-button" text="Play"/>
    <Button fx:id="questionnaireButton" onAction="#pressQuestionnairesButton" styleClass="menu-button"
            text="Questionnaires"/>
    <Button fx:id="settingsButton" onAction="#pressSettingsButton" styleClass="menu-button" text="Settings"/>
    <Button fx:id="quitButton" onAction="#pressQuitButton" styleClass="menu-button" text="Quit"/>

    <stylesheets>
        <URL value="@../styles/standard/general.css"/>
        <URL value="@../styles/standard/menu.css"/>
    </stylesheets>
</VBox>
